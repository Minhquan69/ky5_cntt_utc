<!DOCTYPE html>
<html lang="zxx" class="no-js">

<head>
	<!-- Mobile Specific Meta -->
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<!-- Favicon-->
	<link rel="shortcut icon" href="../temp18/img/fav.png">
	<!-- Author Meta -->
	<meta name="author" content="CodePixar">
	<!-- Meta Description -->
	<meta name="description" content="">
	<!-- Meta Keyword -->
	<meta name="keywords" content="">
	<!-- meta character set -->
	<meta charset="UTF-8">
	<!-- Site Title -->
	<title>Karma Shop</title>
	<!--
		CSS
		============================================= -->
	<link rel="stylesheet" href="../temp18/css/linearicons.css">
	<link rel="stylesheet" href="../temp18/css/font-awesome.min.css">
	<link rel="stylesheet" href="../temp18/css/themify-icons.css">
	<link rel="stylesheet" href="../temp18/css/bootstrap.css">
	<link rel="stylesheet" href="../temp18/css/owl.carousel.css">
	<link rel="stylesheet" href="../temp18/css/nice-select.css">
	<link rel="stylesheet" href="../temp18/css/nouislider.min.css">
	<link rel="stylesheet" href="../temp18/css/ion.rangeSlider.css" />
	<link rel="stylesheet" href="../temp18/css/ion.rangeSlider.skinFlat.css" />
	<link rel="stylesheet" href="../temp18/css/magnific-popup.css">
	<link rel="stylesheet" href="../temp18/css/main.css">
</head>

<body>

	@await Html.PartialAsync("Header")
	@await Html.PartialAsync("FeaturesArea")
	

	
	
	<!-- start product Area -->
	<section class="owl-carousel active-product-area section_gap">
		<!-- single product slide -->
		<div class="single-product-slider">
			<div class="container">
				@RenderBody()
			</div>
		</div>
		<!-- single product slide -->
		
	</section>
	<!-- end product Area -->
	
	<!-- Start brand Area -->
	<section class="brand-area section_gap">
		<div class="container">
			<div class="row">
				<a class="col single-img" href="#">
					<img class="img-fluid d-block mx-auto" src="../temp18/img/brand/1.png" alt="">
				</a>
				<a class="col single-img" href="#">
					<img class="img-fluid d-block mx-auto" src="../temp18/img/brand/2.png" alt="">
				</a>
				<a class="col single-img" href="#">
					<img class="img-fluid d-block mx-auto" src="../temp18/img/brand/3.png" alt="">
				</a>
				<a class="col single-img" href="#">
					<img class="img-fluid d-block mx-auto" src="../temp18/img/brand/4.png" alt="">
				</a>
				<a class="col single-img" href="#">
					<img class="img-fluid d-block mx-auto" src="../temp18/img/brand/5.png" alt="">
				</a>
			</div>
		</div>
	</section>
	<!-- End brand Area -->
	
	@await Html.PartialAsync("Footer")

	<script src="../temp18/js/vendor/jquery-2.2.4.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.../temp18/js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
			crossorigin="anonymous"></script>
	<script src="../temp18/js/vendor/bootstrap.min.js"></script>
	<script src="../temp18/js/jquery.ajaxchimp.min.js"></script>
	<script src="../temp18/js/jquery.nice-select.min.js"></script>
	<script src="../temp18/js/jquery.sticky.js"></script>
	<script src="../temp18/js/nouislider.min.js"></script>
	<script src="../temp18/js/countdown.js"></script>
	<script src="../temp18/js/jquery.magnific-popup.min.js"></script>
	<!--gmaps Js-->
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
	<script src="../temp18/js/gmaps.min.js"></script>
	<script src="../temp18/js/main.js"></script>
</body>

</html>