<!DOCTYPE html>
<html>
<head>
	<title>Online Shopping</title>
	<!-- for-mobile-apps -->
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta name="keywords" content="Super Market, Online Shopping" />

	<!-- //for-mobile-apps -->
	<link href="../De3/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
	<link href="../De3/css/style.css" rel="stylesheet" type="text/css" media="all" />
	<link rel="stylesheet" type="text/css" href="../De3/css/jquery-ui-1.css">
	<!-- font-awesome icons -->
	<link href="../De3/css/font-awesome.css" rel="stylesheet">
	<!-- //font-awesome icons -->
	<!-- js -->
	<script src="../De3/js/jquery-1.11.1.min.js"></script>
	<!-- //js -->
</head>

<body>
	@await Html.PartialAsync("DaoMinhQuan_Header")
	@await Html.PartialAsync("DaoMinhQuan_MainNav")
	<!--- main content --->
	<div class="products">
		<div class="container">
			<!-- left categories -->
			<div class="col-md-4 products-left">
				<div class="categories">
					<h2>Đào Minh Quânnn</h2>
					<ul class="cate">
						@await Component.InvokeAsync("Nav")
					</ul>
				</div>
				<br />
				<a asp-action="ThemProduct">Create Product</a>
			</div>

			<!-- /left categories -->
			<!-- right products grid -->
			
			@RenderBody()
					
			<div class="clearfix"> </div>
			<!-- right products grid -->
		</div>
	</div>
	<!--- main content --->
	@await Html.PartialAsync("DaoMinhQuan_Footer")
</body>
</html>