@model IEnumerable<TSach>
@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}
@foreach(var item in Model)
{
	<div class="grid_1_of_4 images_1_of_4">
		<h4><a href="preview.html">@item.TenSach</a></h4>
		<a href="preview.html"><img src="@Url.Content("../Images/" + @item.FileAnh)" alt="" /></a>
		<div class="price-details">
			<div class="price-number">
				<p><span class="rupees">@item.TacGia</span></p>
			</div>
			<div class="add-cart">
				<h4><a href="preview.html">Xoa sach</a></h4>
			</div>
			<div class="clear"></div>
		</div>
	</div>
}
