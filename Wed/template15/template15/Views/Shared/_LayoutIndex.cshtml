
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Multi Shop</title>
    <!-- Font -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" rel="stylesheet">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="../template15/assets/fonts/fontawesome-free-5.15.3-web/css/all.min.css">
    <!-- Bootstrap -->
    <link href="../template15/assets/bootstrap-5.0.2-dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Animate -->
    <link rel="stylesheet" href="../template15/assets/css/animate.min.css" />
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="../template15/assets/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="../template15/assets/css/owl.theme.default.min.css" />
    <!-- CSS -->
    <link rel="stylesheet" href="../template15/assets/css/base.css">
    <link rel="stylesheet" href="../template15/assets/css/main.css">
    <link rel="stylesheet" href="../template15/assets/css/index.css">
</head>
<body>
    @await Html.PartialAsync("Header")
    @RenderBody() 
    <!-- Categories -->
    <div class="container-fluid pt-5">
        <h2 class="title position-relative text-dark text-uppercase mx-xl-5 mb-4">
            <span class="bg-secondary pe-3">CẦU THỦ</span>
        </h2>
        <div class="row px-xl-5 pb-3">
            @RenderSection("DanhSachCauThu", required: true)

        </div>
    </div>
    @await Html.PartialAsync("footer")
    <!-- Bootstrap -->
    <script src="../template/assets/bootstrap-5.1.3-dist/js/bootstrap.bundle.min.js"></script>
    <!-- Jquery -->
    <script src="../template15/assets/js//jquery-3.6.0.min.js"></script>
    <!-- Owl Carousel -->
    <script src="../template15/assets/js//owl.carousel.min.js"></script>
    <!-- Script -->
    <script src="../template15/assets/js//index.js"></script>
</body>
</html>